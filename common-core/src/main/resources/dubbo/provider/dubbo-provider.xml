<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://code.alibabatech.com/schema/dubbo    
           http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<bean id="dubboConfig"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="order" value="1" />
		<property name="ignoreUnresolvablePlaceholders" value="true" />
		<property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_OVERRIDE" />
<!-- 		<property name="locations" value="classpath:dubbo/dubbo.properties" /> -->
	</bean>
	<dubbo:application name="${dubbo.appname}" />

	<dubbo:registry protocol="${dubbo.registry.protocol}"
		address="${dubbo.registry.address}" file="${dubbo.registry.file}" />
		
	<dubbo:protocol name="${common.dubbo.protocol}" port="${common.dubbo.protocol.port}"></dubbo:protocol>
	<dubbo:protocol name="${common.rest.protocol}" port="${common.rest.protocol.port}"
		server="${dubbo.protocol.server}" contextpath="${dubbo.protocol.contextpath}"
		threads="500" accepts="500" />
	<dubbo:provider timeout="${dubbo.provider.timeout}"
		retries="0" threads="300" />
	<!-- <dubbo:application name="${dubbo.appname}"/> -->
	<!-- <dubbo:registry protocol="${dubbo.registry.protocol}" address="${dubbo.registry.address}" 
		file="${dubbo.registry.file}"/> -->

	<!-- <dubbo:protocol name="${dubbo.protocol}" port="${general.common.dubbo.port}" -->
	<!-- server="${dubbo.protocol.server}" -->
	<!-- contextpath="${dubbo.protocol.contextpath}" -->
	<!-- threads="500" accepts="500" -->
	<!-- /> -->
	<!-- <dubbo:provider timeout="${dubbo.provider.timeout}" retries="0" threads="300"/> -->

	<dubbo:monitor protocol="registry" />

	<dubbo:annotation package="com.x.common" />

	<import resource="../../context/core-context.xml" />

	<!-- <import resource="../../context/elasticjob-context.xml" /> -->
</beans>